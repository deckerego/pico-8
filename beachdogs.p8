function _init()
 x=64
 y=64
 frame=0
end

function _update()
 if(btn(⬅️)) x-=1
 if(btn(➡️)) x+=1
 if(btn(⬆️)) y-=1
 if(btn(⬇️)) y+=1

 if(x%4 == 0) frame = (frame + 1) % 5

 if(btnp(❎)) sfx(0)
end

function _draw()
 cls()
 map(0,0)
 spr(frame*2,x,y,2,2)
end
