pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
function _init()
 dog = {}
 dog.speed = 1.25
 dog.x = 0
 dog.y = 0
 dog.spr = 0
 dog.tick = 0
 dog.west = false
end

function _update()
 if(btn(⬅️)) move_left(dog)
 if(btn(➡️)) move_right(dog)
 if(btn(⬆️)) move_up(dog)
 if(btn(⬇️)) move_down(dog)
 if(btnp(❎)) action(dog)

 if btn() == 0 then
  idle(dog)
 else
  frameadv(dog)
 end
end

function _draw()
 cls()
 map(0, 0)
 spr(dog.spr * 2, dog.x, dog.y, 2, 2, dog.west, false)
end

function idle(plr)
 plr.spr = 0
 plr.tick = 0
end

function move_up(plr)
 plr.y -= 1
end

function move_down(plr)
 plr.y += 1
end

function move_right(plr)
 if not collision(plr.x + 15 + plr.speed, plr.y) then
  plr.x += plr.speed
 end

 plr.west = false
end

function move_left(plr)
 if not collision(plr.x - plr.speed, plr.y) then
  plr.x -= plr.speed
 end

 plr.west = true
end

function frameadv(plr)
 plr.tick += 1
 if(plr.tick % 3 == 0) plr.spr = (plr.spr + 1) % 5
end

function action(plr)
 sfx(0)
end

function collision(x, y)
 local colx = x / 8
 local coly = y / 8
 local cell = mget(colx, coly)
 return fget(cell, 0)
end
